{
  "name": "tw2113/upallnightmovies",
  "description": "My plugins",
  "repositories": [{
    "type": "composer",
    "url": "https://wpackagist.org/"
  }],
  "require": {
    "wpackagist-plugin/disable-emojis": "1.7.2",
    "wpackagist-plugin/query-monitor": "3.7.1",
    "wpackagist-plugin/zero-spam": "5.0.12",
    "wpackagist-plugin/wp-to-twitter": "3.5.3",
    "wpackagist-plugin/semantic-linkbacks": "3.10.4",
    "wpackagist-plugin/wp-uf2": "1.1.0",
    "wpackagist-plugin/indieweb": "3.4.5",
    "wpackagist-plugin/jsonfeed": "1.4.1",
    "wpackagist-plugin/custom-post-type-ui": "1.9.2",
    "wpackagist-plugin/cmb2": "2.9.0",
    "wpackagist-plugin/autodescription": "4.1.4",
    "wpackagist-plugin/ewww-image-optimizer": "6.1.9",
    "wpackagist-plugin/wp-graphql": "1.5.4",
    "wpackagist-plugin/wp-piwik": "1.0.25",
    "wpackagist-plugin/blogroll-block": "1.2.0",
    "wpackagist-plugin/publishpress": "3.4.1"
  },
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": ["type:wordpress-plugin"],
      "themes/{$name}/": ["type:wordpress-theme"],
      "mu-plugin/{$name}/": ["type:wordpress-muplugin"]
    }
  }
}
